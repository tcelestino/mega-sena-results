{
  "name": "mega-sena-results",
  "version": "0.0.0",
  "description": "the latest mega sena results",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tcelestino/mega-sena-results.git"
  },
  "author": "Tiago Celestino",
  "license": "MIT",
  "engines": {
    "node": ">=12",
    "npm": ">=6"
  },
  "scripts": {
    "dev": "nodemon src/*.js",
    "lint": "eslint src --ext .js --max-warnings 3",
