    "scraping": "node src/index.js",
    "send": "node src/send.js"
  },
  "devDependencies": {
    "eslint": "8.57.1",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.30.0",
    "eslint-plugin-prettier": "5.2.1",
    "lint-staged": "15.2.10",
    "nodemon": "3.1.4",
    "prettier": "3.3.3"
  },
  "lint-staged": {
    "*.{js}": [
      "yarn lint"
    ]
  },
  "dependencies": {
    "dotenv": "16.4.5",
    "nodemailer": "6.9.15",
    "puppeteer": "23.3.0"
  }
}
