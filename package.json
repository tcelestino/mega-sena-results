{
  "name": "mega-sena-results",
  "version": "0.0.0",
  "description": "the latest mega sena results",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tcelestino/mega-sena-results.git"
  },
  "author": "Tiago Celestino",
  "license": "MIT",
  "engines": {
    "node": ">=12",
    "npm": ">=6"
  },
  "scripts": {
    "dev": "node-dev src/*.js --notify=false",
    "lint": "eslint src --ext .ts,.js --max-warnings 3",
    "start": "npm run scraping && npm run send",
    "scraping": "node src/index.js",
    "send": "node src/send.js",
    "checktypes": "tsc"
  },
  "devDependencies": {
    "@types/node": "14.14.14",
    "@types/puppeteer": "3.0.2",
    "@typescript-eslint/eslint-plugin": "4.0.1",
    "@typescript-eslint/parser": "4.0.1",
    "eslint": "7.8.1",
    "eslint-config-airbnb-typescript": "^10.0.2",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-prettier": "3.1.4",
    "lint-staged": "10.3.0",
    "node-dev": "5.2.0",
    "prettier": "2.2.1",
    "typescript": "4.1.3"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "npm run format"
    ]
  },
  "dependencies": {
    "dotenv": "8.2.0",
    "nodemailer": "6.4.11",
    "puppeteer": "5.3.0"
  }
}
