{
  "name": "mega-sena-results",
  "version": "0.0.0",
  "description": "the latest mega sena results",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tcelestino/mega-sena-results.git"
  },
  "author": "Tiago Celestino",
  "license": "MIT",
  "engines": {
    "node": ">=12",
    "npm": ">=6"
  },
  "scripts": {
    "dev": "node-dev src/*.js --notify=false",
    "lint": "eslint src --ext .ts,.js --max-warnings 3",
    "start": "npm run scraping && npm run send",
    "scraping": "node src/index.js",
    "send": "node src/send.js"
  },
  "devDependencies": {
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.22.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-prettier": "3.4.0",
    "lint-staged": "10.5.4",
    "node-dev": "6.3.0",
    "prettier": "2.2.1"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "npm run format"
    ]
  },
  "dependencies": {
    "dotenv": "8.2.0",
    "nodemailer": "6.4.18",
    "puppeteer": "7.0.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
